<div id="logo-nav">
  <a href="/" class="logo">
  	<img src="/image/scribble.png" title="<%= config.title %>"/>
  </a>
  <h1><%= config.title %></h1>
  <h2><% if (config.subtitle){ %><%= config.subtitle %><% } %></h2>
</div>
<nav id="main-nav">
  <ul>
    <% for (var i in theme.menu){ %>
      <li><a href="<%- theme.menu[i] %>"><%= i %></a></li>
    <% } %>
  </ul>
  <div class="clearfix"></div>
</nav>